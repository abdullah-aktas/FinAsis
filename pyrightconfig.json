{
    "include": [
        "**/*.py"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/venv",
        "**/.venv"
    ],
    "ignore": [
        "**/site-packages"
    ],
    "reportMissingImports": false,
    "pythonVersion": "3.13",
    "pythonPlatform": "Windows",
    "typeCheckingMode": "basic"
} 